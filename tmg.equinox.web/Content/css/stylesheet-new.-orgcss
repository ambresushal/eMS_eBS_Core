  top: 3px;
}
.custom-combobox .ui-button-icon-only{
  position: absolute;
  right: 8px;
  top: 5px;
  border: none;
  width: auto;
}

/* Calender CSS */
.ui-datepicker{
  width: auto;
}
.ui-datepicker table{
  background: #fff;
  margin-bottom: 0;
}
#ui-datepicker-div {
    box-shadow: 3px 2px 7px 1px rgba(0,0,0,0.2);
}
.ui-datepicker .ui-datepicker-header{
  padding: 10px 0;
  background-color: #55C39E;
}
.ui-datepicker .ui-datepicker-prev, .ui-datepicker .ui-datepicker-next{
  border: none;
}
.ui-datepicker .ui-datepicker-prev span,
.ui-datepicker .ui-datepicker-next span{
  font-family: "Material Icons";
  background: transparent;
  text-indent: 0;
  color: #fff;
  font-size: 21px;
  height: 25px;
}
.ui-datepicker .ui-datepicker-next span{
  transform: rotate(90deg);
}
.ui-datepicker .ui-datepicker-prev span{
  transform: rotate(-90deg);
}
.ui-datepicker .ui-datepicker-next span:before{
  content: "\E5C7";
}
.ui-datepicker .ui-datepicker-prev span:before{
  content: "\E5C7";
}
.ui-datepicker-trigger{
  width: 0;
  height: 0;
}
.ui-datepicker-trigger:before{
  content: "\E8DF";
  width: 25px;
  height: 25px;
}
.ui-datepicker-header .ui-datepicker-week-end{
  font-size: 16px;
  color: #233752;
}
.ui-datepicker td .ui-state-hover{
  background: #233752;
}
.ui-widget .ui-datepicker-week-end,
.ui-widget.ui-datepicker th{
  font-size: 20px;
  color: #233752;
}
.ui-datepicker .ui-datepicker-prev span{
  margin-top: -2px;
}
.ui-datepicker-calendar tr,
.ui-datepicker-calendar td,
.ui-widget.ui-datepicker td{
  border: none;
}
.ui-datepicker td .ui-state-default{
  font-size: 20px;
}
.ui-datepicker select.ui-datepicker-month,
.ui-datepicker select.ui-datepicker-year {
    width: 48%;
    padding: 4px;
    border-radius: 5px;
}
.ui-datepicker select.ui-datepicker-month{
  margin-right: 5px;
}

.ui-tabs .ui-tabs-nav.white-bg, .white-bg {
  background: #fff;
}
.ui-tabs .ui-tabs-nav.white-bg li {
  top:1px;
  border-bottom:4px solid transparent !important;
}
.ui-tabs .ui-tabs-nav.white-bg li.ui-state-active {
  top:1px;
  border-bottom:4px solid #57BD99 !important;
}
.subsection .row{
  margin-bottom:10px;
}
.subsection .subsections .panel-title {
  background:none;
  padding-bottom: 10px;
  border-color:#ccc;
}
.details-custom-pagination >  button{
  color: #243754 !important;
  padding: 0px 10px !important;
  border-left: 1px solid #243754;
  border-radius: 0;
  margin-top: 10px;
}
.details-custom-pagination >  button:first-child { 
  border-left: none !important
}
.details-custom-pagination >  button:hover{
  background:none
}
#sectionMenuContainer.sectionMenuFromContainer .form-text{
  width: 100px;
  padding: 15px;
  float: left;
}
#sectionMenuContainer.sectionMenuFromContainer .form-fields{
  width: calc(100% - 170px);
  float: left;
  margin-top: 10px;
}
