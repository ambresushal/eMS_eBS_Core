}
.round label {
  background-color: #fff;
  border: 1px solid #57bd98;
  border-radius: 50%;
  cursor: pointer;
  height: 18px;
  left: 0;
  position: absolute;
  top: 0;
  width: 18px;
}

.round label:after {
  border: 2px solid #a4a5aa;
  border-top: none;
  border-right: none;
  content: "";
  height: 6px;
  left: 3px;
  opacity: 0;
  position: absolute;
  top: 4px;
  transform: rotate(-45deg);
  width: 10px;
}

.round input[type="checkbox"] {
  visibility: hidden;
}
.round input[type="checkbox"]:checked + label {
  /*background-color: #A4A5AA;*/
}
.round input[type="checkbox"]:checked + label:after {
  opacity: 1;
  border-color: #57bd98;
}
.round input[type="checkbox"]:disabled + label:after {
    border-color: #a4a5aa;
}
