.bwizard-steps li::before {
    content: "";
    position: absolute;
    width: 29px;
    height: 29px;
    background:transparent;
    border: 1px solid #ccc;
    border-width: 0 1px 1px 0;
    top: 5px;
    left: -15px;
    transform: rotate(-45deg);
  }
  .bwizard-steps li.approved, .bwizard-steps li.approved + li::before {
    background:#fff;
  }
