@{
    ViewBag.Title = "Reporting Center";
    Layout = "~/Views/Shared/ReportingCenterLayout.cshtml";
}
<div class="panel panel-default">
    <div class="panel-body">
        <div class="row-fluid">
            <div class="col-xs-12 col-md-8">
                <table id="workQueue"></table>
            </div>
            <div class="col-xs-12 col-md-4">
                <table id="formUpdates"></table>
            </div>
        </div>
        <div class="row-fluid">

            <div class="col-sm-12 col-xs-12 col-lg-12">
                <table id="watch"></table>
            </div>
        </div>
    </div>
</div>

<div style="display: none;">
    <div id="viewAssignedUserDialog" style="width: auto; height: auto;">
        <div>
            <div class="form-group row">
                <table id="viewAssignedUsers"></table>
            </div>
        </div>
    </div>
</div>

<div style="display: none;">
    <div id="UserAssignUnassignDialog">
        <div>
            <div class="form-group row">
                <table id="userAssignmentUpdate"></table>
            </div>
            @* <button type="button" id="saveUserAssignment" class="btn btn-default" value="Save"></button>*@
        </div>
    </div>
</div>

<script type="text/javascript">
    var vbIsFolderLockEnable = @Html.Raw(ViewBag.IsFolderLockEnable);
    var claims = @Html.Raw(Json.Encode(@ViewBag.Claims));
    var vbRole = @Html.Raw(Json.Encode(ViewBag.RoleId));
    setMenu("dashBoard");
</script>

