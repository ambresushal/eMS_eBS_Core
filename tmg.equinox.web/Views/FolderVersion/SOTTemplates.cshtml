<script id="SOTLabelTemplate" type="text/x-jsrender">

</script>

<script id="SOTTextAreaTemplate" type="text/x-jsrender">
    <tr class="ui-widget-content sotview-ltr" data-group="{{>~group}}" data-filter="{{>ElementID}}">
