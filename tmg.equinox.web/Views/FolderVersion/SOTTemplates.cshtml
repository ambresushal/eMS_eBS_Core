                          maxlength='{{>MaxLength}}' class='richtext-editorformat form-control hastooltip {{>SpellCheck}} {{if Enabled === false}} disabled {{/if}}' {{if Enabled === false}} disabled="disabled" {{/if}} data-link='{{>FullName}}' data-journal='{{>FullName}}'>
                    {{>DefaultValue}}
                </textarea>
            </div>
        </td>
        {{/if}}
