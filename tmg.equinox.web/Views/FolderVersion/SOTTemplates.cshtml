</script>

<script id="SOTInputElementTemplate" type="text/x-jsrender">
    <tr class="ui-widget-content sotview-ltr" data-group="{{>~group}}" data-filter="{{>ElementID}}">
        {{if ~isElementApplicable(EffDt,Op) === false}}
