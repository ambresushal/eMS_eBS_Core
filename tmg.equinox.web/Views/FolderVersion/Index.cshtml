        SectionGeneratedName:'@TempData["SectionGeneratedName"]'
    };

    if(folderData.isPortfolio == "True" || folderData.isPortfolio == true){
        setMenu("PortfolioSearch");
    }
    else if( folderData.folderType !== "MasterList"){
        setMenu("account");
    }else{
        setMenu("masterList");
    }



    $(document).ready(function () {
        var viewMode = folderData.FolderViewMode;
        //execute the singleton object at document.ready event
        var singleTonfolderVersionInstance = new folderVersionInstance.getInstance(folderData.accountId, folderData.folderVersionId, folderData.versionNumber, folderData.effectiveDate, folderData.folderId, folderData.referenceProductFormInstanceID, folderData.isEditable, folderData.folderType,viewMode, folderData.currentUserId,folderData.currentUserName);
        singleTonfolderVersionInstance.load();
        if (GLOBAL.applicationName.toLowerCase() != 'ebenefitsync') {
            if(folderData.folderVersionState == 1) {
                folderVersionInstance.checkIfEVCompletedForFolderversion(folderData.folderVersionId, folderData.folderId, folderData.isEditable, folderData.folderType, viewMode, folderData.currentUserId,folderData.currentUserName);
            }
        }
    });

    var vbIsHiddenOrDisableSections = @Html.Raw(ViewBag.IsHiddenOrDisableSections);
