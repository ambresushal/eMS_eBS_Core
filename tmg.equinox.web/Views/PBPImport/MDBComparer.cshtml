                                <div style="margin-top:20px;"></div>
                                <div class="col-sm-4">
                                    <label for="exportMDBFile"><b>Exported File: </b></label><em>*</em>
                                </div>
                                <div class="col-xs-8">
                                    <input id="exportMDBFileName" name="exportMDBFileName" class="form-control uploadTmplFile" type="file" />
                                    <span class="help-block" id="exportMDBFileNameSpan" style="color:red"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-2" style="padding-left: 70px;">
                            <div style="margin-top:20px;"></div>
                            <button id="btnMDBCompare" class="btn pull-left" type="button">Compare</button>
                        </div>
                    </div>
                    <div class="row">
                       
                    </div>
                </div>
            </div>
