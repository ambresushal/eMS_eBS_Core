        <ul class="nav nav-pills nav-pills-child border-none" role="tablist">
            <li><a href="#mdbComparerTab-1">MDB Comparer</a></li>
        </ul>
        <div id="mdbComparerTab-1" class="tab-pane active">

            <div class="panel panel-default ">
                <div class="panel-body">
                    <div class="row">
                        <div class="col-xs-5">
                            <div class="row">
                                <div style="margin-top:10px;"></div>
                                <div class="col-sm-4">
                                    <label for="importMDBFile"><b>Imported File: </b></label><em>*</em>
